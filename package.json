{
  "name": "cerium",
  "version": "0.3.0",
  "description": "Project Description",
  "homepage": "https://project-domain.tld",
  "repository": {
    "type": "git",
    "url": "https://github.com/chancedigital/cerium"
  },
  "author": {
    "name": "Chance Strickland",
    "email": "hi@chancedigital.io",
    "url": "https://chancedigital.io",
    "role": "developer"
  },
  "scripts": {
    "setup": "yarn && composer install && composer install-codestandards",
    "start": "yarn && composer install && gulp --gulpfile config/gulpfile.babel.js",
    "dev": "NODE_ENV=development gulp dev --gulpfile config/gulpfile.babel.js",
    "build": "NODE_ENV=production gulp build --gulpfile config/gulpfile.babel.js",
    "build:staging": "NODE_ENV=staging gulp build --gulpfile config/gulpfile.babel.js",
    "lint-js": "eslint assets/js",
    "format-js": "eslint --fix assets/js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.4",
    "@babel/preset-env": "7.5.4",
    "@babel/register": "7.4.4",
    "@chancedigital/eslint-config": "2.0.2",
    "@chancedigital/stylelint-config": "2.0.5",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-inline-json-import": "0.3.2",
    "babel-plugin-lodash": "3.3.4",
    "browserslist": "4.6.6",
    "caniuse-db": "1.0.*",
    "cssnano": "^4.1.10",
    "dateformat": "3.0.3",
    "del": "5.0.0",
    "dotenv-webpack": "1.7.0",
    "eslint": "6.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.9.0",
    "eslint-plugin-react": "7.14.0",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-wpcalypso": "4.1.0",
    "gulp": "4.0.2",
    "gulp-cache": "1.1.2",
    "gulp-filter": "6.0.0",
    "gulp-imagemin": "6.0.0",
    "gulp-livereload": "4.0.1",
    "gulp-notify": "3.2.0",
    "gulp-phpcs": "3.0.0",
    "gulp-postcss": "8.0.0",
    "gulp-rename": "1.4.0",
    "gulp-sass": "git+https://github.com/chancestrickland/gulp-sass",
    "gulp-sourcemaps": "2.6.5",
    "gulp-zip": "5.0.0",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "node-sass": "4.13.1",
    "node-sass-tilde-importer": "1.0.2",
    "postcss-preset-env": "6.7.0",
    "prettier": "git+https://github.com/Automattic/wp-prettier#wp-prettier-1.17.0",
    "pump": "3.0.0",
    "require-dir": "1.2.0",
    "stylelint": "10.1.0",
    "stylelint-scss": "3.9.1",
    "terser-webpack-plugin": "1.3.0",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.6",
    "webpack-merge": "4.2.1",
    "webpack-stream": "5.2.1",
    "webpackbar": "3.2.0"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "lodash": "4.17.19",
    "what-input": "5.2.3"
  },
  "engineStrict": true,
  "engines": {
    "node": "~10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "eslintConfig": {
    "extends": "@chancedigital/eslint-config/wp",
    "globals": {
      "jQuery": false,
      "wp": false
    },
    "rules": {
      "no-console": [
        1,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "no-debugger": 1
    }
  },
  "eslintIgnore": [
    "assets/js/_vendor",
    "assets/js/*/_vendor",
    "dist",
    "tests"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "parenSpacing": true,
    "trailingComma": "all"
  },
  "stylelint": {
    "extends": [
      "@chancedigital/stylelint-config/scss",
      "@chancedigital/stylelint-config/wp"
    ],
    "ignore": [
      "**/*.js"
    ]
  }
}
